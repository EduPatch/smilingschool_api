                                            "$ref": "#/components/schemas/appType"
                                        },
                                        "state": {
                                            "$ref": "#/components/schemas/notificationState"
                                        },
                                        "type": {
                                            "type": "string"
                                        },
                                        "url": {
                                            "type": "string"
                                        },
                                        "orderDate": {
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "pupilIM2Id": {
                                            "type": "integer"
                                        },
                                        "pupilSourceId": {
                                            "type": "string"
                                        },
                                        "currentlySelectedPupil": {
                                            "type": "boolean"
                                        },
                                        "entityTypeString": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/NotificationApp/NotificationApp/UpdateNotificationState": {
