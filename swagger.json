                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "default": {
                        "description": "Responds with a success value of true or false",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/authentication/authentication/isauthenticated": {
            "post": {
                "tags": [
                    "authentication"
                ],
                "description": "Retreive if user is authenticated or not",
                "parameters": [
                    {
                        "name": "_",
                        "in": "path",
                        "description": "Parameter to provide a timestamp. Purpose is unknown"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Always returns 200, but the content actually determines if it was successful or not!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
